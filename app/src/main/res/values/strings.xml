<resources>

    <!-- Name of the application. [CHAR_LIMIT=NONE] -->
    <string name="app_name">Calculator</string>
    <!-- Label shown in Quick Settings tile picker/list. -->
    <string name="quick_settings_tile_label" translatable="false">Calculator</string>
    <!-- Title for the launcher settings screen that controls the Quick Settings tile. -->
    <string name="quick_settings_settings_title" translatable="false">Quick Settings Tile</string>
    <!-- Description for the launcher settings screen. -->
    <string name="quick_settings_settings_description" translatable="false">Use this screen to add the Calculator tile to Quick Settings.</string>
    <!-- Action for requesting tile addition to Quick Settings. -->
    <string name="quick_settings_add_tile_action" translatable="false">Add Tile to Quick Settings</string>
    <!-- Temporary debug action to open a standard app dialog with the same blur settings. -->
    <string name="quick_settings_open_debug_dialog" translatable="false">Open Standard Debug Dialog</string>
