    <string name="quick_settings_add_tile_result_bad_component" translatable="false">Tile component is not available.</string>
    <!-- Label used for launcher entry. -->
    <string name="quick_calc_launcher_name" translatable="false">Quick Calc</string>
    <!-- Settings section heading for theme controls. -->
    <string name="settings_section_theme" translatable="false">Theme</string>
