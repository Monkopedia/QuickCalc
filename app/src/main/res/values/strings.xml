    <string name="quick_settings_debug_dialog_title" translatable="false">Standard App Dialog Debug</string>
    <!-- Result shown when adding tiles is unsupported on this Android version. -->
    <string name="quick_settings_add_tile_requires_tiramisu" translatable="false">Adding tiles from the app requires Android 13 or newer.</string>
    <!-- Result shown when StatusBarManager is unavailable. -->
    <string name="quick_settings_add_tile_status_bar_unavailable" translatable="false">Quick Settings is not available right now.</string>
